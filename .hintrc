{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "detect-css-reflows/composite": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "animation-timeline",
          "animation-timeline: view()",
          "animation-range"
        ]
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/paint": "off"
  }
}